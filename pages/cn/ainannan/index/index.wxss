page{
  background-color: #f2efe6;
}

.titleViews{
  width: 90%;
  height: 140px;
  margin: auto;
  position: absolute;
  top: -65%;
  left: 0;
  right: 0;
  bottom: 0;
}

.titleText{
  font-size: 24px;
}

.titleView{
  height: 90px;
  border-radius: 3%/9%;
  background-color: #ddd;
  padding-top: 18px;
  padding-left: 18px;
  padding-right: 18px;
}

.voteView{
  text-align: right;
  font-size: 15px;
  color:#aaa;
  height: 20px;
}

.buttonView{
  text-align: center;
  width: 85%;
  height: 170px;
  margin: auto;
  position: absolute;
  top: 70%;
  left: 0;
  right: 0;
  bottom: 0;
}

.nextView{
  width: 100px;
  height: 100px;
  background-color: #44B549;
  color: #fff;
  margin: auto;
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  line-height: 100px; /*设置line-height与父级元素的height相等*/
}

.collectshare{
  width: 70px;
  height: 70px;
  background-color: #cccc33;
  line-height: 70px; /*设置line-height与父级元素的height相等*/
}

.collectView{
  margin: auto;
  position: absolute;
  top: 0;
  left: -80%;
  right: 0;
  bottom: 0;
}

.shareView{
  margin: auto;
  position: absolute;
  top: 0;
  left: 78%;
  right: 0;
  bottom: 0;
}

.circle{
  border-radius: 50%;
  text-align: center; /*设置文本水平居中*/
  overflow: hidden; /*防止内容超出容器或者产生自动换行*/
}

/*弹出层*/
.modal-box{
  position:fixed;
  width:100%;
  height:100%;
  top:0px;
  background:rgba(0,0,0,0.4);
  overflow: hidden;
  }

.modal-body{
  position:relative;
  }

.modal-content{
  /* width: 60%; */
  margin:0 10% 0 30%;
  overflow: hidden;
  border-radius: 10rpx;}

.modal-content{
  width: 60%;
  height:150px; 
}
.modal-content .fenxiang{
  width: 100%;
  height: 40%;
}

.detailView{
  width: 91%;
  height: 13%;
  margin: auto;
  position: absolute;
  top: 56%;
  left: 3%;
  color:#666;
  /* background-color: #f3fff7; */
  background:-webkit-gradient(linear, 0 0, 0 bottom, from(#f2efe6), to(rgba(255, 255, 255, 0.8)));
  padding-left: 10px;
}

.detailText{
  width:90%;
  height: 42%;
  overflow: hidden;
}

#clickGetMore{
  margin-left: 32%;
  font-size: 15px;
}